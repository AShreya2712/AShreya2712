def has_pair_with_sum(arr, K):
    # Create a dictionary to store the frequency of each number
    freq = {}
    
    for num in arr:
        if K - num in freq:
            return True
        # Increase the frequency of the current number
        if num in freq:
            freq[num] += 1
        else:
            freq[num] = 1
    
    return False

def main():
    n = int(input())  # Read the length of the list
    arr = list(map(int, input().split()))  # Read the list elements
    K = int(input())  # Read the target sum
    
    if has_pair_with_sum(arr, K):
        print("Yes")
    else:
        print("No")

if __name__ == "__main__":
    main()
