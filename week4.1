N=int(intput())
non_repeated_count=0
digit_occurences=[0]*10
temp_N=N
while temp_N>0:
     digit =temp_N%10
     digit_occurences[digit] +=1
     temp_N //=10
temp_N=N
while temp_N>0:
     digit = temp_N%10
     if digit_occurences[digit] == 1:
        digit_occurences[digit] ==-1
        non_repeated_count += 1
     temp_N  //=10
print(non_repeated_count)
